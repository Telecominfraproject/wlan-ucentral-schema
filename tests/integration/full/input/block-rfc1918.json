{
	"uuid": 2,
	"radios": [
		{
			"band": "2G",
			"country": "CA",
			"channel-mode": "HE",
			"channel-width": 20,
			"channel": 1
		}
	],

	"interfaces": [
		{
			"name": "WAN",
			"role": "upstream",
			"ethernet": [
				{
					"select-ports": [
						"WAN*"
					]
				}
			],
			"ipv4": {
				"addressing": "dynamic"
			}
		},
		{
			"name": "LAN",
			"role": "downstream",
			"services": [ "ssh" ],
			"ethernet": [
				{
					"select-ports": [
						"LAN*"
					]
				}
			],
			"ipv4": {
				"addressing": "static",
				"subnet": "192.168.1.1/24",
				"dhcp": {
					"lease-first": 10,
					"lease-count": 100,
					"lease-time": "6h"
				},
				"disallow-upstream-subnet": [
					"192.168.0.0/16",
					"127.16.0.0/12",
					"10.0.0.0/8"
				]
			},
			"ssids": [
				{
					"name": "OpenWifi",
					"wifi-bands": [
						"2G"
					],
					"bss-mode": "ap",
					"encryption": {
						"proto": "psk2",
						"key": "OpenWifi",
						"ieee80211w": "optional"
					}
				}
			]

		}
	],
	"services": {
		"ssh": {
			"port": 22
		}
	}
}
