# generated by ieee8021x.uc
### generate ieee8021x base configuration
add ieee8021x config
set ieee8021x.@config[-1].ca='/etc/ssl/ca.pem'
set ieee8021x.@config[-1].cert='/etc/ssl/cert.pem'
