{
  "uuid": 1,
  "interfaces": [
    {
      "name": "downstream1",
      "role": "downstream",
      "services": [
        "dhcp-snooping"
      ],
      "ethernet": [
        {
          "select-ports": [
            "LAN1",
            "LAN2"
          ]
        }
      ],
      "ipv4": {
        "addressing": "static",
        "subnet": "192.168.1.1/24"
      }
    },
    {
      "name": "downstream2",
      "role": "downstream",
      "services": [
        "dhcp-snooping"
      ],
      "ethernet": [
        {
          "select-ports": [
            "LAN3",
            "LAN4"
          ]
        }
      ],
      "ipv4": {
        "addressing": "static",
        "subnet": "192.168.2.1/24"
      }
    },
    {
      "name": "WAN",
      "role": "upstream",
      "services": [
        "dhcp-snooping"
      ],
      "ethernet": [
        {
          "select-ports": [
            "WAN"
          ]
        }
      ],
      "ipv4": {
        "addressing": "dynamic"
      }
    }
  ],
  "services": {
    "dhcp-snooping": {
      "interfaces": [
        "downstream1",
        "downstream2",
        "upstream"
      ]
    }
  },
  "metrics": {
    "dhcp-snooping": {
      "filters": [
        "ack",
        "discover",
        "offer"
      ]
    }
  }
}
