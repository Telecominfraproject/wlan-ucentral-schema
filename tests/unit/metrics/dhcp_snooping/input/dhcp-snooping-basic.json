{
  "uuid": 1,
  "interfaces": [
    {
      "name": "WAN",
      "role": "upstream",
      "ethernet": [
        {
          "select-ports": [
            "WAN"
          ]
        }
      ],
      "ipv4": {
        "addressing": "dynamic"
      }
    },
    {
      "name": "downstream",
      "role": "downstream",
      "services": [
        "dhcp-snooping"
      ],
      "ethernet": [
        {
          "select-ports": [
            "LAN*"
          ]
        }
      ],
      "ipv4": {
        "addressing": "static",
        "subnet": "192.168.1.1/24",
        "dhcp": {
          "lease_first": 10,
          "lease_count": 100
        }
      }
    }
  ],
  "services": {
    "dhcp-snooping": {
      "interfaces": [
        "downstream"
      ]
    }
  },
  "metrics": {
    "dhcp-snooping": {
      "filters": [
        "ack",
        "discover",
        "offer",
        "request"
      ]
    }
  }
}
