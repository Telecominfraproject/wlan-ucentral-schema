            {
              "protocol": "udp",
              "port": 5060,
              "range_end": 5070,
              "reclassify": false
            }
          ],
          "dns": [
            {
              "fqdn": "sip.company.com",
              "suffix_matching": true,
              "reclassify": false
            }
          ]
        },
        {
          "dscp": "AF41",
          "ports": [
            {
              "protocol": "any",
              "port": 1935,
              "reclassify": true
            }
          ]
        }
      ]
    }
