{
  "uuid": 1,
  "interfaces": [
    {
      "name": "WAN",
      "role": "upstream",
      "ethernet": [
        {
          "select-ports": [
            "WAN"
          ]
        }
      ],
      "ipv4": {
        "addressing": "dynamic"
      }
    },
    {
      "name": "downstream",
      "role": "downstream",
      "ethernet": [
        {
          "select-ports": [
            "LAN*"
          ]
        }
      ],
      "ipv4": {
        "addressing": "static",
        "subnet": "192.168.1.1/24"
      }
    }
  ],
  "services": {
    "dhcp-relay": true
  },
  "dhcp_relay": {
    "select_ports": [
      "eth0"
    ],
    "vlans": [
      {
        "vlan": 100,
        "relay_server": "192.168.100.1"
      }
    ]
  }
}
