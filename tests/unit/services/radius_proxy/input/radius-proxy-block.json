{
  "uuid": 1,
  "interfaces": [
    {
      "name": "WAN",
      "role": "upstream",
      "ethernet": [
        {
          "select-ports": [
            "WAN"
          ]
        }
      ],
      "ipv4": {
        "addressing": "dynamic"
      }
    },
    {
      "name": "downstream",
      "role": "downstream",
      "services": [
        "radsecproxy"
      ],
      "ssids": [
        {
          "name": "Blocked-WiFi",
          "wifi-bands": [
            "2G"
          ],
          "bss-mode": "ap",
          "encryption": {
            "proto": "wpa2",
            "mode": "wpa2-enterprise"
          },
          "services": [
            "radsecproxy"
          ]
        }
      ]
    }
  ],
  "services": {
    "radius_proxy": {
      "proxy_secret": "block-secret",
      "realms": [
        {
          "protocol": "block",
          "realm": [
            "blocked.example.com",
            "*.blocked.com"
          ],
          "message": "Access denied - realm blocked"
        }
      ]
    }
  }
}
