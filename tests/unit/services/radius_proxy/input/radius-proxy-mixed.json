{
  "uuid": 1,
  "interfaces": [
    {
      "name": "WAN",
      "role": "upstream",
      "ethernet": [
        {
          "select-ports": [
            "WAN"
          ]
        }
      ],
      "ipv4": {
        "addressing": "dynamic"
      }
    },
    {
      "name": "downstream",
      "role": "downstream",
      "services": [
        "radsecproxy"
      ],
      "ssids": [
        {
          "name": "Enterprise-WiFi",
          "wifi-bands": [
            "2G",
            "5G"
          ],
          "bss-mode": "ap",
          "encryption": {
            "proto": "wpa3",
            "mode": "wpa3-enterprise"
          },
          "services": [
            "radsecproxy"
          ]
        }
      ]
    }
  ],
  "services": {
    "radius_proxy": {
      "proxy_secret": "mixed-proxy-secret",
      "realms": [
        {
          "protocol": "radsec",
          "realm": [
            "secure.example.com"
          ],
          "auto_discover": true,
          "use_local_certificates": true
        },
        {
          "protocol": "radius",
          "realm": [
            "legacy.example.com"
          ],
          "auth_server": "192.168.2.100",
          "auth_port": 1812,
          "auth_secret": "legacy-auth-secret"
        },
        {
          "protocol": "block",
          "realm": [
            "banned.example.com"
          ],
          "message": "Realm access denied"
        }
      ]
    }
  }
}
