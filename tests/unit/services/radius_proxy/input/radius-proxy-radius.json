{
  "uuid": 1,
  "interfaces": [
    {
      "name": "WAN",
      "role": "upstream",
      "ethernet": [
        {
          "select-ports": [
            "WAN"
          ]
        }
      ],
      "ipv4": {
        "addressing": "dynamic"
      }
    },
    {
      "name": "downstream",
      "role": "downstream",
      "services": [
        "radsecproxy"
      ],
      "ssids": [
        {
          "name": "Guest-WiFi",
          "wifi-bands": [
            "2G"
          ],
          "bss-mode": "ap",
          "encryption": {
            "proto": "wpa2",
            "mode": "wpa2-enterprise"
          },
          "services": [
            "radsecproxy"
          ]
        }
      ]
    }
  ],
  "services": {
    "radius_proxy": {
      "proxy_secret": "guest-proxy-secret",
      "realms": [
        {
          "protocol": "radius",
          "realm": [
            "guest.example.com"
          ],
          "auth_server": "192.168.1.100",
          "auth_port": 1812,
          "auth_secret": "auth-secret-123",
          "acct_server": "192.168.1.100",
          "acct_port": 1813,
          "acct_secret": "acct-secret-456"
        }
      ]
    }
  }
}
