{
  "uuid": 1,
  "interfaces": [
    {
      "name": "WAN",
      "role": "upstream",
      "ethernet": [
        {
          "select-ports": [
            "WAN"
          ]
        }
      ],
      "ipv4": {
        "addressing": "dynamic"
      }
    },
    {
      "name": "downstream",
      "role": "downstream",
      "ethernet": [
        {
          "select-ports": [
            "LAN*"
          ]
        }
      ],
      "ipv4": {
        "addressing": "static",
        "subnet": "192.168.1.1/24"
      },
      "vlan": {
        "id": 100
      },
      "ssids": [
        {
          "name": "PublicWiFi",
          "wifi-bands": [
            "2G",
            "5G"
          ],
          "bss-mode": "ap",
          "encryption": {
            "proto": "none"
          },
          "services": [
            "captive"
          ]
        }
      ]
    }
  ],
  "services": {
    "captive": {
      "auth-mode": "click-to-continue",
      "walled-garden-fqdn": [
        "google.com",
        "facebook.com"
      ],
      "walled-garden-ipaddr": [
        "8.8.8.8",
        "1.1.1.1"
      ]
    }
  }
}
