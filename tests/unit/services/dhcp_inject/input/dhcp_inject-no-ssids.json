{
  "uuid": 1,
  "interfaces": [
    {
      "name": "WAN",
      "role": "upstream",
      "ethernet": [
        {
          "select-ports": [
            "WAN"
          ]
        }
      ],
      "ipv4": {
        "addressing": "dynamic"
      }
    },
    {
      "name": "downstream",
      "role": "downstream",
      "ethernet": [
        {
          "select-ports": [
            "LAN*"
          ]
        }
      ],
      "ipv4": {
        "addressing": "static",
        "subnet": "192.168.1.1/24"
      },
      "ssids": [
        {
          "name": "TestWiFi",
          "wifi-bands": [
            "2G"
          ],
          "bss-mode": "ap",
          "encryption": {
            "proto": "psk2",
            "key": "testpassword123"
          }
        }
      ]
    }
  ],
  "services": {
    "dhcp-inject": true
  },
  "dhcp_inject": {
    "select_ports": [
      "eth0"
    ]
  }
}
