set rrmd.@base[0].beacon_request_assoc=0
set rrmd.@base[0].station_stats_interval=120
# RRM policy configuration for Optimization based on Channel Utilization
	add rrmd policy
	set rrmd.@policy[-1].name='chanutil'
	set rrmd.@policy[-1].interval=600000
	set rrmd.@policy[-1].threshold=80
	set rrmd.@policy[-1].consecutive_threshold_breach=3
	set rrmd.@policy[-1].algo=1