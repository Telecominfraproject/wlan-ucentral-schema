{
  "uuid": 1,
  "interfaces": [
    {
      "name": "WAN",
      "role": "upstream",
      "ethernet": [
        {
          "select-ports": [
            "WAN"
          ]
        }
      ],
      "ipv4": {
        "addressing": "dynamic"
      }
    },
    {
      "name": "downstream",
      "role": "downstream",
      "services": [
        "rrmd"
      ],
      "wifi": [
        {
          "band": "2G",
          "channel": "auto",
          "bandwidth": 20
        }
      ]
    }
  ],
  "services": {
    "rrm": {
      "beacon_request_assoc": false,
      "station_stats_interval": 120,
      "chanutil": {
        "interval": 600,
        "threshold": 80,
        "consecutive_threshold_breach": 3,
        "algo": "rcs"
      }
    }
  }
}
