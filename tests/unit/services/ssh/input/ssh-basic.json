{
  "uuid": 1,
  "interfaces": [
    {
      "name": "WAN",
      "role": "upstream",
      "services": [
        "ssh"
      ],
      "ethernet": [
        {
          "select-ports": [
            "WAN"
          ]
        }
      ],
      "ipv4": {
        "addressing": "dynamic"
      }
    },
    {
      "name": "downstream",
      "role": "downstream",
      "services": [
        "ssh"
      ],
      "ethernet": [
        {
          "select-ports": [
            "LAN*"
          ]
        }
      ],
      "ipv4": {
        "addressing": "static",
        "subnet": "192.168.1.1/24"
      }
    }
  ],
  "services": {
    "ssh": {
      "port": 22,
      "password_authentication": true,
      "idle_timeout": 300,
      "authorized_keys": [
        "ssh-rsa AAAAB3NzaC1yc2EAAAADAQABAAABAQC... user@example.com",
        "ssh-ed25519 AAAAC3NzaC1lZDI1NTE5AAAAI... admin@example.com"
      ]
    }
  }
}
